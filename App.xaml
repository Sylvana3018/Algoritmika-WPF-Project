<Application x:Class="algoritm.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:algoritm"
             xmlns:converters="clr-namespace:algoritm.Converters"
             xmlns:fa="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <local:PercentageToWidthConverter x:Key="PercentageToWidthConverter" />
        <local:PercentageToFixedWidthConverter x:Key="PercentageToFixedWidthConverter" />
        <converters:AttendanceToColorConverter x:Key="AttendanceToColorConverter" />
        
        
    </Application.Resources>
</Application>
